    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "nws": "^1.1.1",
    "prettier": "^2.2.1",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-property-sort-order-smacss": "^7.0.0",
    "stylelint-config-recommended": "^4.0.0",
